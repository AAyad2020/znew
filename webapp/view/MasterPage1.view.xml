					<!--	</attributes>-->
					<!--	<firstStatus>-->
					<!--		<ObjectStatus text="Submitted" state="Warning" visible="true"/>-->
					<!--	</firstStatus>-->
					<!--	<secondStatus/>-->
					<!--</ObjectListItem>-->
					<!--<ObjectListItem intro="Company code" title="6666" number="12.02.2003" numberUnit="" numberState="None" type="Active" selected="false"-->
					<!--	showMarkers="false">-->
					<!--	<attributes>-->
					<!--		<ObjectAttribute title="Document Type" text="ziew" active="false" visible="true"/>-->
					<!--	</attributes>-->
					<!--	<firstStatus>-->
					<!--		<ObjectStatus text="Rejected" state="Error" visible="true"/>-->
					<!--	</firstStatus>-->
					<!--	<secondStatus/>-->
					<!--</ObjectListItem>-->
				</items>
			</List>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Add Request" type="Default" icon="sap-icon://add-document" iconFirst="true" width="auto" enabled="true" visible="true"
						iconDensityAware="false" press="_onButtonPress"/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
	</Page>
</mvc:View>