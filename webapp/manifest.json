{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.sap.jvPosting",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.2.2"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZGW_FI_JEPOSTING_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"ZGW_FI_JEREPOSTNEW_SRV": {
				"uri": "/sap/opu/odata/sap/ZGW_FI_JEREPOSTNEW_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			},
			"ZGW_FI_JEREPOSTFI_SRV": {
				"uri": "/sap/opu/odata/sap/ZGW_FI_JEREPOSTFI_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZGW_FI_JEREPOSTFI_SRV/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		}
	},
	"sap.fiori": {
		"_version": "1.1.0",
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.3.0",
		"handleValidation": true,
		"rootView": {
			"viewName": "com.sap.jvPosting.view.App",
			"type": "XML",
			"async": false,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": false,
			"cozy": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sap.jvPosting.view",
				"controlId": "idAppControl",
				"clearTarget": false,
				"controlAggregation": "detailPages",
				"async": false,
				"bypassed": {
					"target": [
						"MasterPage1",
						"notFound"
					]
				}
			},
			"targets": {
				"MasterPage1": {
					"controlAggregation": "masterPages",
					"viewName": "MasterPage1",
					"viewId": "MasterPage1",
					"viewLevel": 1,
					"transition": "slide"
				},
				"DetailPage1": {
					"controlAggregation": "detailPages",
					"viewName": "DetailPage1",
					"viewId": "DetailPage1",
					"viewLevel": 3,
					"transition": "slide"
				},
				"NewRequest": {
					"controlAggregation": "detailPages",
					"viewName": "NewRequest",
					"viewId": "NewRequest",
					"viewLevel": 2,
					"transition": "slide"
				},
				"notFound": {
					"controlAggregation": "detailPages",
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "MasterPage1",
					"target": [
						"MasterPage1",
						"DetailPage1"
					]
				},
				{
					"pattern": "DetailPage1/{ObjectId}",
					"name": "DetailPage1",
					"target": [
						"MasterPage1",
						"DetailPage1"
					]
				},
				{
					"pattern": "MasterPage1/NewRequest",
					"name": "NewRequest",
					"target": [
						"MasterPage1",
						"NewRequest"
					]
				}
			]
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZGW_FI_JEREPOSTFI_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zbsp_jvposting/webapp",
		"_version": "1.1.0"
	}
}